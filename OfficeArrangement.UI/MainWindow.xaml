<Window x:Class="OfficeArrangement.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OfficeArrangement.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="22" VerticalAlignment="Top" Width="517" Background="#FFFFFFFF">
            <MenuItem Header="File">
                <MenuItem Header="Load..." HorizontalAlignment="Left" Width="140" Click="MenuItem_Click"/>
            </MenuItem>
        </Menu>
        <Image x:Name="imageMap"
               HorizontalAlignment="Stretch"
               VerticalAlignment="Stretch"
               Width="auto"
               Height="auto"
               Margin="10,26,10,10" />
    </Grid>
</Window>
